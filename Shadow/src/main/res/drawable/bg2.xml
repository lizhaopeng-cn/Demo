<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="45dp" android:height="45dp">
        <shape android:shape="rectangle">
            <gradient android:type="linear" android:useLevel="true" android:startColor="#ffff8b7c" android:endColor="#ffff5346" android:angle="135" />
            <corners android:radius="2dp" />
            <item android:name="android:shadowColor">#80ff5346</item>
            <item android:name="android:shadowDx">0</item>
            <item android:name="android:shadowDy">1</item>
        </shape>
    </item>
</layer-list>